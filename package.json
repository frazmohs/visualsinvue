{
  "name": "visualsinvue",
  "version": "1.2.47",
  "description": "Interactive chord and tree visualization widget",
  "main": "index.js",
  "jest": {
    "testMatch": [
      "<rootDir>/**/**"
    ],
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "scripts": {
    "start": "webpack-dev-server --open --config ./webpack/webpack-dev.config.js",
    "test": "jest",
    "build": "rm ./dist/*.js; rm ./dist/umd/*.js; webpack --config ./webpack/webpack.config.js",
    "ship": "cp package.json dist/; cd dist/; npm publish; git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frazmohs/visualsinvue"
  },
  "keywords": [
    "data visualization",
    "chord visualization",
    "tree visualization"
  ],
  "files": [
    "umd",
    "dist",
    "src"
  ],
  "author": "Sayyed Faraz Mohseni",
  "license": "Faraz",
  "homepage": "https://github.com/frazmohs/visualsinvue",
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "jest": "^27.5.1",
    "jsdom": "9.12.0",
    "jsdom-global": "2.1.1",
    "node-sass": "^4.5.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.2",
    "sass-loader": "^6.0.3",
    "should": "^11.2.1",
